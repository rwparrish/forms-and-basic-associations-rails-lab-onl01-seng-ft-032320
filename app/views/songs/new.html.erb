<h1>Create a Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>

  <%= f.label :song_artist_name %>
  <%= f.text_field :artist_name %><br>

  <%= f.label :song_genre_id %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
 
  <%= f.fields_for :notes do |n| %>
    <%= n.label :content %>
    <%= n.text_field :content %><br>
 
  <% end %>
 
  <%= f.submit %>
<% end %>


